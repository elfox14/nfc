\n};\ndocument.addEventListener(\"DOMContentLoaded\", () => App.init());\n","usedDeprecatedRules":[{"ruleId":"no-extra-semi","replacedBy":[]},{"ruleId":"no-mixed-spaces-and-tabs","replacedBy":[]}]},{"filePath":"C:\\Users\\TheFo\\Downloads\\nfc\\script-safe-fixes.js","messages":[{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":29,"column":21,"nodeType":"Identifier","messageId":"undef","endLine":29,"endColumn":34},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":42,"column":23,"nodeType":"Identifier","messageId":"undef","endLine":42,"endColumn":36},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":48,"column":18,"nodeType":"Identifier","messageId":"undef","endLine":48,"endColumn":31},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":54,"column":18,"nodeType":"Identifier","messageId":"undef","endLine":54,"endColumn":31},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":59,"column":21,"nodeType":"Identifier","messageId":"undef","endLine":59,"endColumn":34},{"ruleId":"no-undef","severity":2,"message":"'showToast' is not defined.","line":72,"column":15,"nodeType":"Identifier","messageId":"undef","endLine":72,"endColumn":24},{"ruleId":"no-undef","severity":2,"message":"'showToast' is not defined.","line":76,"column":15,"nodeType":"Identifier","messageId":"undef","endLine":76,"endColumn":24},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":85,"column":22,"nodeType":"Identifier","messageId":"undef","endLine":85,"endColumn":35},{"ruleId":"no-undef","severity":2,"message":"'createElement' is not defined.","line":106,"column":18,"nodeType":"Identifier","messageId":"undef","endLine":106,"endColumn":31},{"ruleId":"no-undef","severity":2,"message":"'DragManager' is not defined.","line":114,"column":47,"nodeType":"Identifier","messageId":"undef","endLine":114,"endColumn":5