{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "warn"
    },
    "globals": {
        "Config": "readonly",
        "DOMElements": "readonly",
        "Utils": "readonly",
        "StateManager": "readonly",
        "HistoryManager": "readonly",
        "CardManager": "readonly",
        "UIManager": "readonly",
        "ExportManager": "readonly",
        "GalleryManager": "readonly",
        "ShareManager": "readonly",
        "EventManager": "readonly",
        "TabManager": "readonly",
        "TourManager": "readonly",
        "Shepherd": "readonly",
        "html2canvas": "readonly",
        "QRCode": "readonly",
        "JSZip": "readonly",
        "gtag": "readonly"
    }
}